<%= form_for(@animal, :html => {:multipart => true}) do |f| %>
	<%= f.label "Animal name: " %>
	<%= f.text_field :name %>

	<%= f.label "Animal image: " %>
	<%= f.file_field :images, :multiple => true %>

	<%= f.submit "Create Animal" %>
<% end %>